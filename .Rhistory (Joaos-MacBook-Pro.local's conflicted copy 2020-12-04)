nminmaxmeanmedsd <- function(x){
y <- x[!is.na(x)]
n_test <- length(y)
min_test <- min(y)
max_test <- max(y)
mean_test <- mean(y)
med_test <- median(y)
sd_test <- sd(y)
out <- c(n_test, min_test, max_test, mean_test, med_test, sd_test)
names(out) <- c("n", "min", "max", "mean", "median", "sd")
return(out)
}
